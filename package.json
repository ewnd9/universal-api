  "license": "MIT",
  "dependencies": {
    "bluebird": "^2.10.0",
    "detect-node": "^2.0.3",
    "superagent": "^1.4.0",
    "superagent-jsonp": "0.0.5"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "chai": "^3.2.0",
    "mocha": "^2.2.5"
  }
}
